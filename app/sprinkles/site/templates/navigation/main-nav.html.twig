<div class="container">
    <nav class="navbar navbar-default">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                    data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a href="{{ site.uri.public }}">
                <img src="https://www.meritdental.com/wp-content/themes/midwest-2016/images/MidwestDental_logo.png"
                     alt="logo" class="logo">
            </a>
        </div>

        {% if checkAuthenticated() %}
        {% include 'navigation/search.html.twig' %}
        {% endif %}

        <div class="mobile clear"></div>
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

                    <!-- /.navbar-collapse -->
            <!-- Navbar Right Menu -->
            <div class="navbar-custom-menu navbar-right">
                <ul class="nav navbar-nav">
                    <li><a href="{{ site.uri.public }}">Home</a></li>

                    {% block secondary_nav %}
                        {% if not checkAuthenticated() %}
                            <li>
                                <a href="{{ site.uri.public }}/account/sign-in"
                                   class="nav-highlight">{{ translate("SIGNIN") }}</a>
                            </li>
                        {% else %}
                            {% include "navigation/user-card.html.twig" %}
                        {% endif %}
                    {% endblock %}
                </ul>
            </div>
            <!-- /.navbar-custom-menu -->

        </div><!-- /.navbar-collapse -->

    </nav>
</div>

